{
  "cli_name": "codex",
  "display_name": "OpenAI Codex CLI",
  "current_supported_version": "2.0.0",
  "minimum_version": "1.5.0",
  "last_checked": "2025-11-16",
  "last_updated": "2025-11-16",
  "sources": {
    "official_docs": "https://openai.com/docs/codex-cli",
    "github_repo": "https://github.com/openai/codex-cli",
    "changelog": "https://github.com/openai/codex-cli/releases",
    "npm_package": "https://www.npmjs.com/package/@openai/codex-cli",
    "api_reference": "https://platform.openai.com/docs/api-reference"
  },
  "install_methods": {
    "npm": "npm install -g @openai/codex-cli",
    "homebrew": "brew install codex",
    "winget": "winget install OpenAI.Codex"
  },
  "verification": {
    "install_check": "which codex || command -v codex",
    "version_command": "codex --version",
    "version_pattern": "codex version (\\d+\\.\\d+\\.\\d+)",
    "health_check": "codex --help"
  },
  "requirements": {
    "api_key_env": "OPENAI_API_KEY",
    "min_node_version": "16.0.0",
    "os": ["linux", "macos", "windows"]
  },
  "breaking_changes": {
    "2.0.0": [
      "sandbox 모드 필수화",
      "exec 명령어 구문 변경",
      "resume 옵션 추가"
    ],
    "1.5.0": [
      "config 명령어 추가",
      "model 선택 옵션 변경"
    ]
  },
  "update_check_interval_days": 7,
  "auto_update_enabled": false
}
